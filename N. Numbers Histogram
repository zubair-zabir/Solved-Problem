/*https://codeforces.com/group/MWSDmqGsZm/contest/219432/problem/N*/
#include <bits/stdc++.h>
using namespace std;
int main()
{
    char c;
    cin >> c;
    int i, n;
    cin >> n;
    for(i = 1; i <= n; i++)
    {
        int j, k;
        cin >> j;
        for(k = 1; k <= j; k++)
        {
            cout << c;
        }
        cout << endl;
    }
}
